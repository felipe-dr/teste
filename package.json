{
  "name": "project-name",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "keywords": [
    "node",
    "typescript",
    "jest",
    "supertest",
    "eslint",
    "prettier",
    "husky",
    "git-commit-msg-linter"
  ],
  "main": "main.js",
  "author": {
    "name": "Felipe DR",
    "email": "felipe.corp7@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felipe-dr/project-name.git"
  },
  "bugs": {
    "url": "https://github.com/felipe-dr/project-name/issues",
    "email": "felipe.corp7@gmail.com"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc --project tsconfig-build.json && tsc-alias",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only --clear --require tsconfig-paths/register src/main.ts",
    "husky:prepare": "husky install",
    "lint": "eslint {src,e2e}/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "lint:fix:staged": "eslint --fix",
    "prettier": "prettier {src,e2e}/**/*.ts",
    "prettier:format": "npm run prettier -- --write",
    "prettier:format:staged": "prettier --write",
    "test": "jest --passWithNoTests --colors --runInBand --no-cache",
    "test:unit": "npm test -- --watch --config ./jest-unit.config.js",
    "test:e2e": "npm test -- --watch --config ./jest-e2e.config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
